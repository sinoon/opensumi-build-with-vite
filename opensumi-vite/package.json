{
    "name": "opensumi-vite",
    "version": "0.0.1",
    "main": "dist/index.js",
    "scripts": {
        "dev": "NODE_ENV=development webpack serve --config webpack.config.dev.js",
        "clean": "rimraf dist",
        "tsc": "cross-env NODE_ENV=production tsc",
        "build:webpack": "cross-env NODE_ENV=production webpack build",
        "build:vite": "vite build",
        "build": "npm run clean && npm run tsc && npm run build:vite"
    },
    "files": [
        "dist",
        "package.json"
    ],
    "browser": {
        "stream": false,
        "indexof": false,
        "crypto": false
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "babel": {
        "presets": [
            "react-app"
        ]
    },
